{
  "name": "DLink cameras",
  "version": "1.0.0",
  "slug": "dlink_cameras",
  "description": "DLink cameras manager",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "application",
  "boot": "auto",
  "homeassistant_api": true,
  "url": "https://github.com/vladimirfriptu/ha_dlink_cameras",
  "ports": {
    "4001/tcp": 4001
  },
  "map": ["config:rw"],
  "options": {
    "debug": false,
    "isSwaggerEnabled": false,
    "logs_directory_path": "/config/www/camera_events",
    "logs_parsing": {
      "motion_match_name": "MOTION",
      "sound_match_name": "SOUND"
    },
    "sensor_live_delay": {
      "motion": 50000,
      "sound": 50000
    },
    "sensors": {
      "motion": "kitchen_motion_camera",
      "sound": "kitchen_sound_camera"
    },
    "cameras": [
      {
        "entityId": "",
        "host": "",
        "username": "",
        "password": "",
        "createevent_motion_on": "",
        "createevent_motion_off": "",
        "createevent_sound_on": "",
        "createevent_sound_off": ""
      }
    ]
  },
  "schema": {
    "debug": "bool",
    "isSwaggerEnabled": "bool",
    "logs_directory_path": "str?",
    "logs_parsing": {
      "motion_match_name": "str?",
      "sound_match_name": "str?"
    },
    "sensor_live_delay": {
      "motion": "int?",
      "sound": "int?"
    },
    "sensors": {
      "motion": "str",
      "sound": "str"
    },
    "cameras": [
      {
        "entityId": "str",
        "host": "str",
        "username": "str",
        "password": "str",
        "createevent_motion_on": "str",
        "createevent_motion_off": "str",
        "createevent_sound_on": "str",
        "createevent_sound_off": "str"
      }
    ]
  }
}
